{
  "name": "react-native-fs2",
  "version": "3.0.0",
  "description": "Native filesystem access for react-native",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "*.podspec",
    "!example",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "lint": "eslint \"src/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "example:android": "cd example; yarn run android",
    "example:ios": "cd example; yarn run ios"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sourcetoad/react-native-fs2.git"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "fs",
    "filesystem",
    "download"
  ],
  "contributors": [
    "Johannes Lumpe <johannes@lum.pe> (https://github.com/johanneslumpe)",
    "Hagen HÃ¼bel <hhuebel@itinance.com> (https://github.com/itinance)",
    "Connor Tumbleson <connor@sourcetoad.com> (https://github.com/iBotPeaches)"
  ],
  "license": "MIT",
  "dependencies": {
    "base-64": "^1.0.0",
    "utf8": "^3.0.0"
  },
  "devDependencies": {
    "@react-native-community/bob": "^0.17.1",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/base-64": "^1.0.0",
    "@types/react": "^18.0.26",
    "@types/react-native": "^0.70.7",
    "@types/utf8": "^3.0.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "metro-react-native-babel-preset": "^0.73.4",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "eslintConfig": {
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
